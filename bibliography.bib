@article{Wagner2021.10.12.464122,
	abstract = {Large-scale datasets present unique opportunities to perform scientific investigations with un-precedented breadth. However, they also pose considerable challenges for the findability, accessibility, interoperability, and reusability (FAIR) of research outcomes due to infrastructure limitations, data usage constraints, or software license restrictions. Here we introduce a DataLad-based, domain-agnostic framework suitable for reproducible data processing in compliance with open science mandates. The framework attempts to minimize platform idiosyncrasies and performance-related complexities. It affords the capture of machine-actionable computational provenance records that can be used to retrace and verify the origins of research outcomes, as well as be re-executed independent of the original computing infrastructure. We demonstrate the framework{\textquoteright}s performance using two showcases: one highlighting data sharing and transparency (using the studyforrest.org dataset) and another highlighting scalability (using the largest public brain imaging dataset available: the UK Biobank dataset).Competing Interest StatementThe authors have declared no competing interest.},
	author = {Wagner, Adina S. and Waite, Laura K. and Wierzba, Ma{\l}gorzata and Hoffstaedter, Felix and Waite, Alexander Q. and Poldrack, Benjamin and Eickhoff, Simon B. and Hanke, Michael},
	doi = {10.1101/2021.10.12.464122},
	elocation-id = {2021.10.12.464122},
	eprint = {https://www.biorxiv.org/content/early/2022/02/01/2021.10.12.464122.full.pdf},
	journal = {bioRxiv},
	publisher = {Cold Spring Harbor Laboratory},
	title = {FAIRly big: A framework for computationally reproducible processing of large-scale data},
	url = {https://www.biorxiv.org/content/early/2022/02/01/2021.10.12.464122},
	year = {2022},
	Bdsk-Url-1 = {https://www.biorxiv.org/content/early/2022/02/01/2021.10.12.464122},
	Bdsk-Url-2 = {https://doi.org/10.1101/2021.10.12.464122}}

